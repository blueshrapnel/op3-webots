<?xml version="1.0" encoding="UTF-8"?>
<root BTCPP_format="4">
  <BehaviorTree ID="test_walk">
    <Sequence>
      <ForceSuccess>
        <Timeout msec="2000">
          <Repeat num_cycles="-1">
            <Twist x="0.5"
                   y="0.0"
                   yaw="0.0"/>
          </Repeat>
        </Timeout>
      </ForceSuccess>
      <ForceSuccess>
        <Timeout msec="2000">
          <Repeat num_cycles="-1">
            <Twist x="0.0"
                   y="0.0"
                   yaw="0.3"/>
          </Repeat>
        </Timeout>
      </ForceSuccess>
      <ForceSuccess>
        <Timeout msec="2000">
          <Repeat num_cycles="-1">
            <Twist x="0.5"
                   y="0.0"
                   yaw="0.0"/>
          </Repeat>
        </Timeout>
      </ForceSuccess>
    </Sequence>
  </BehaviorTree>

  <!-- Description of Node Models (used by Groot) -->
  <TreeNodesModel>
    <Action ID="Twist">
      <input_port name="x"
                  default="0.0"/>
      <input_port name="y"
                  default="0.0"/>
      <input_port name="yaw"
                  default="0.0"/>
    </Action>
  </TreeNodesModel>

</root>
